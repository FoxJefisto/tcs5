<panel >
	<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="0"/>

	<container>
		<attachment_group class="vertical_arrange" vertical_alignment="top, extend" />

		<container >
			<layout class="horizontal_arrange" horizontal_layout="500,-1"  />

			<container  class="tcs_box">
				<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />

				<container height="18">
					<layout class="stick_to_sides_attachment" alignment="left" inset="0" apply_to_children="true" />
					<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />

					<!-- Linked header -->
					<widget class="text" font="title" section="Pfrm" auto_size="all">
						<translation id="text" translation_id="395324" type="use" value="Form (Last 5 Matches)[COMMENT: player overview; sub panel title for player form graph]" />
					</widget>

					<widget class="spacer" height="3" />
				</container>

				<!--debug-->
				<container class="scrolling_box" id="tttt">
					<widget class="client_object_label_value_table" >
						<record id="object_property">
							<integer id="get_property" value="objt" />
							<integer id="set_property" value="objt"/>
						</record>

						<record id="table_properties" mode="fill_rows, stripe_rows" auto_size_rows="true" row_height="24" row_spacing="4"/>
						<list id="viewed_property_ids">
							<record id="Snat" />
							<!--Nation-->
							<record id="SctI" />
							<!--City-->
							<record id="SctW" />
							<!--City weather-->
							<record id="Slat" />
							<!--Latitude-->
							<record id="Slon" />
							<!--Longitude-->

							<!--Under Construction-->
							<record id="SblC" />
							<!--Cost of new stadium-->
							<record id="SblS" />
							<!--Build start date-->
							<record id="SblC" />
							<!--Build end date-->
							<record id="Sbdy" />
							<!--Year built-->
							<record id="Srby" />
							<!--Year rebuilt-->
							<record id="Scpy" />
							<!--Capacity-->
							<record id="Sscy" />
							<!--Seated Capacity-->
							<record id="Sucy" />
							<!--Used Capacity-->
							<record id="Scas" />
							<!--Capacity for all seated games-->
							<record id="Secy" />
							<!--Expansion capacity-->
							<record id="TstD" />
							<!--Stadium capacity description -->
							<record id="Soty" />
							<!--Owner type-->
							<record id="Sown" />
							<!--Owning club-->
							<record id="Doco" />
							<!--Date of ownership-->
							<record id="Snft" />
							<!--Named after reason-->
							<record id="Sren" />
							<!--Rent-->
							<record id="SsfT" />
							<!--Surface type-->
							<record id="Splr" />
							<!--Pitch last relaid-->
							<record id="Sdcy" />
							<!--Stadium condition-->
							<record id="Senv" />
							<!--Stadium nearby stadium-->
							<record id="Sush" />
							<!--Under soil heating-->
							<record id="Scov" />
							<!--Roof-->
							<record id="Srrf" />
							<!--Retractable roof-->
							<record id="Sdah" />
							<!--Digital ad hoardings-->
							<record id="Sovl" />
							<!--Oval-->
							<record id="Sext" />
							<!--Extinct-->
							<record id="Sitg" />
							<!--Training Ground-->
							<record id="Snew" />
							<record id="Sunt" />
							<!--Used by national team-->
							<record id="Su21" />
							<!--Used by national u21 team-->
							<record id="Su19" />
							<!--Used by national u19 team-->
						</list>
					</widget>
				</container>
			</container>

			<!--matches-->
			<container >
				<layout class="vertical_arrange" vertical_layout="-12,-24" />

				<container class="tcs_box" draw_vertical_dividers="true">
					<attachment_group class="horizontal_arrange" horizontal_alignment="fill" horizontal_gap="24" />

					<!-- COMPETITIONS -->
					<container id="comp">
						<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />
						
										<container height="18">
					<layout class="stick_to_sides_attachment" alignment="left" inset="0" apply_to_children="true" />
					<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />

					<!-- Linked header -->
					<widget class="text" font="title" auto_size="all">
						<translation id="text" translation_id="227741" type="use" />
					</widget>

					<widget class="spacer" height="3" />
				</container>

						<widget class="client_object_list_table" id="CmPs" database_table_type="55" fixed_size_rows="false" auto_size_rows="true" row_spacing="4" mode="fill_rows, stripe_rows, printable, store_sort_state">
							<record id="object_property">
								<integer id="get_property" value="Scom" />
								<integer id="set_property" value="objt"/>
							</record>
							<record id="viewed_property_ids">
								<record id="dref" />
							</record>
						</widget>
					</container>

					<!--teams-->
					<container>
						<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />
						
							<container height="18">
					<layout class="stick_to_sides_attachment" alignment="left" inset="0" apply_to_children="true" />
					<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />

					<!-- Linked header -->
					<widget class="text" font="title" auto_size="all">
						<translation id="text" translation_id="229362" type="use" />
					</widget>

					<widget class="spacer" height="3" />
				</container>

						<container class="scrolling_box" id="team" >
							<container class="simple_data_list" id="tems" set_property="objt" auto_size="vertical" gap="6">
								<record id="object_property">
									<integer id="get_property" value="Susr" />
								</record>
								<record id="widget_properties" class="client_object_viewer_xml_panel" file="team/team item panel" />
							</container>
						</container>
					</container>

				</container>


				<container class="tcs_box" id="matc">
					<attachment_group class="vertical_arrange" vertical_alignment="top,extend" />
					
									<container height="18">
					<layout class="stick_to_sides_attachment" alignment="left" inset="0" apply_to_children="true" />
					<layout class="stick_to_sides_attachment" alignment="vertical" inset="0" apply_to_children="true" />

					<!-- Linked header -->
					<widget class="text" font="title" auto_size="all">
						<translation id="text" translation_id="251247" type="use" />
					</widget>

					<widget class="spacer" height="3" />
				</container>
				
					<widget class="client_object_list_table" id="FixL" database_table_type="53" fixed_size_rows="false" auto_size_rows="true" mode="fill_rows, stripe_rows, printable, store_sort_state">
						<record id="object_property">
							<integer id="get_property" value="Slym" />
							<integer id="set_property" value="objt"/>
						</record>

						<!-- default sort -->
						<list id="default_sort_column">
							<integer value="fxdt" />
						</list>

						<!--default view from the view list-->
						<flags id="default_view" value="gene" />

						<!--list of all available views-->
						<record id="viewed_property_ids">
							<flags id="fxdt" />
							<flags id="home" />
							<flags id="FxRs" />
							<flags id="away" />
						</record>

					</widget>
				</container>

			</container>
		</container>

	</container>

</panel>