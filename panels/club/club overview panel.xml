<panel should_force_refresh="true">
	<layout class="vertical_arrange" vertical_layout="200,-1" vertical_gap="8"/>

	<!-- HEADER -->
	<container>
		<attachment_group class="horizontal_arrange" horizontal_alignment="right,extend" horizontal_gap="8"/>

		<widget class="club_overview_details_panel" id="clod" file="club/tcs/overview/clod">
			<record id="object_property" get_property="objt" set_property="objt"/>
		</widget>

		<!-- KITS -->
		<container width="550">
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>
			<translation id="title" translation_id="" type="use" value="Kits[COMMENT: title for the club kits the club overview panel]"/>

			<widget class="club_overview_kits_panel" id="cloK" file="none">
				<record id="object_property" get_property="objt" set_property="objt"/>

				<attachment_group class="horizontal_arrange" horizontal_alignment="fill" horizontal_gap="8"/>

				<record id="default_properties" scale_picture="true" keep_aspect_ratio="true" image_alignment="centre" always_use_latest_object_list="true">
					<record id="object_property">
						<integer id="get_property" value="Tidx"/>
						<integer id="set_property" value="indx"/>
					</record>
				</record>

				<container class="tcs_box" id="T_hk">
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>
					<widget class="kit_picture" id="T_hk" kit_id="0" scale_picture="true" keep_aspect_ratio="true" image_alignment="centre" always_use_latest_object_list="true">
						<record id="object_property">
							<integer id="get_property" value="Tidx"/>
							<integer id="set_property" value="indx"/>
						</record>
					</widget>
				</container>

				<container class="tcs_box" id="T_ak">
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>
					<widget class="kit_picture" id="T_ak" kit_id="1" scale_picture="true" keep_aspect_ratio="true" image_alignment="centre" always_use_latest_object_list="true">
						<record id="object_property">
							<integer id="get_property" value="Tidx"/>
							<integer id="set_property" value="indx"/>
						</record>
					</widget>
				</container>

				<container class="tcs_box" id="T_tk">
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>
					<widget class="kit_picture" id="T_tk" kit_id="2" scale_picture="true" keep_aspect_ratio="true" image_alignment="centre" always_use_latest_object_list="true">
						<record id="object_property">
							<integer id="get_property" value="Tidx"/>
							<integer id="set_property" value="indx"/>
						</record>
					</widget>
				</container>
			</widget>
		</container>

	</container>

	<!-- BOTTOM -->
	<container>
		<layout class="horizontal_arrange" horizontal_layout="-560,-600,-560" horizontal_gap="8"/>

		<!-- LEAGUE TABLE AND RESULTS -->
		<container>
			<layout class="vertical_arrange" vertical_layout="-3,-6" vertical_gap="4"/>

			<!-- RESULTS AND FIXTURES -->
			<container class="vertical_arrange_container" alignment="fill" gap="4">

				<container class="club_overview_team_results_panel" file="club/tcs/club overview team calendar results">
					<record id="object_property" get_property="objt" set_property="objt"/>
				</container>

				<container class="club_overview_team_results_panel" file="club/tcs/club overview team calendar fixtures">
					<record id="object_property" get_property="objt" set_property="objt"/>
				</container>
			</container>

			<!-- LEAGUE TABLE -->
			<container class="tcs_box">
				<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

				<container>
					<layout class="vertical_arrange" vertical_alignment="top,extend" vertical_gap="0"/>

					<widget class="table" layout="-4, -1" row_spacing="2" mode="none" auto_size="vertical" fixed_size_rows="false" auto_size_rows="true">

						<!-- Recent Form -->
						<container class="club_overview_details_panel" file="none" row="0" col="1">
							<layout class="stick_to_sides_attachment" apply_to_children="true" inset="0,0,0,8"/>

							<record id="object_property" get_property="Tftm" set_property="objt"/>

							<widget class="team_recent_form_widget" alignment="right">
								<record id="object_property" get_property="Tfrm" set_property="valu"/>
							</widget>
						</container>

						<!-- Past positions -->
						<widget class="label" auto_size="vertical" size="small" row="0" col="0">
							<translation id="text" translation_id="232109" type="use" value="League Positions"/>
						</widget>

						<!-- League Table -->
						<container class="league_positions_panel" id="lpos" file="team/team competitions overview past positions panel" row="1" col="0" column_span="2" height="40">
							<record id="object_property">
								<list id="get_properties">
									<!-- PROP_TEAM_CURRENT_DIVISION -->
									<record get_property="Cddl" set_property="objt"/>
									<record get_property="Tftm" set_property="team"/>
								</list>
							</record>
						</container>
					</widget>

					<container class="competition_news_panel" file="none">
						<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true"/>

						<record id="object_property" get_property="objt" set_property="objt"/>

						<container class="competition_overview_panel" file="none">
							<layout class="stick_to_sides_attachment" alignment="all"/>

							<record id="object_property" get_property="Cdiv" set_property="objt"/>

							<attachment_group class="horizontal_arrange" horizontal_layout="-1,-2" horizontal_offset="0" vertical_inset="0">
								<global id="horizontal_gap" name="ui.main_contrast_box_gap"/>
							</attachment_group>

							<container id="cost" navigation_container="true" default_focus="false">
								<layout class="stick_to_sides_attachment" apply_to_children="true"/>

								<widget class="competition_stage_panel" file="none">
									<layout class="stick_to_sides_attachment" apply_to_children="true"/>
									<record id="object_property" get_property="objt" set_property="objt"/>
									<boolean id="overview_version" value="true"/>

									<widget class="league_past_positions_panel">
										<record id="object_property" get_property="objt" set_property="objt"/>
									</widget>

									<!-- group box which extends to include all other areas -->
									<container>
										<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="0"/>

										<!-- the group box that is the main container for the competition stage panel this container holds whatever league or cup panel we need to create -->
										<container class="inner_box" id="stse" force_title_borders="false">

											<!-- buttons area -->
											<container embed_in_frame="true">
												<attachment_group class="title_embedded_panel_left" horizontal_offset="4"/>
												<widget class="link_button_title" section="cssp" id="labl" embed_in_frame="true">
													<translation id="text" translation_id="247175" type="use" value="Stages"/>
												</widget>
												<!-- the stages menu: visible if more than one stage/substage -->
												<widget class="view_menu_popup" id="stpb" sele="acsm" auto_tick_exclusive="true" save_session_state="true" embed_in_frame="true" auto_size="horizontal" show_button_appearance="false">
												</widget>
											</container>

											<!-- bottom row of the stage panel is used to store cup draw buttons -->
											<layout class="vertical_arrange" vertical_alignment="bottom,extend"/>

											<!-- No game panel-->
											<container id="nots" hidden="true">
												<layout class="arrange_vertical_attachment" alignment="top" offset="0" gap="0"/>
												<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true"/>
												<layout class="stick_to_sides_attachment" alignment="all" inset="0"/>
												<widget class="spacer" height="10"/>
												<widget class="text" id="nost" alignment="centre" spec="text" size="large" style="semi_bold, embossed" auto_size="vertical"/>
											</container>

											<!-- group box for stage to go into -->
											<container id="stgb">
												<layout class="vertical_arrange" vertical_alignment="top,extend"/>
												<container height="16"/>

												<container>
													<layout class="vertical_arrange" vertical_alignment="top,extend"/>

													<!--league stage panel-->
													<widget class="league_stage_panel" id="lspn" hidden="true" view="ovrC" league_table_heading_length="0"/>
													<!--cup stage_panel-->
													<widget class="cup_stage_panel" file="competition/cup overview stage" id="cstp" hidden="true" save_session_state="true"/>
													<!--group stage scrolling box (to be filled in code)-->
													<widget class="scrolling_box" id="gscb" hidden="true" save_session_state="true" navigation_container="true"/>
												</container>
											</container>

											<container>
												<attachment_group class="vertical_arrange_autosize" vertical_offset="0" vertical_gap="0" horizontal_alignment="horizontal" horizontal_inset="0"/>
												<!-- The exceptions so point deductions in the league etc-->
												<widget class="table" layout="-1" id="exta" auto_size="vertical" auto_size_rows="true" row_spacing="2" hidden="true" mode="fill_rows"/>
											</container>
										</container>
									</container>
								</widget>
							</container>
						</container>
					</container>
				</container>
			</container>
		</container>

		<!-- MIDDLE -->
		<container class="tcs_tabs" id="tclub" default_height="-1" priority="1" default_item="co01" save_default_state="true" save_session_state="true">

	
			<!-- OVERVIEW -->
			<container id="over">
				<layout class="vertical_arrange" vertical_layout="360,-1,-1" vertical_gap="8"/>
				<translation id="title" translation_id="336543" type="use"/>

				<container class="tcs_box">
					<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>
					<widget class="club_overview_stadium_panel" id="clos" file="club/tcs/overview/stadium">
						<record id="object_property">
							<list id="get_properties">
								<record>
									<integer id="get_property" value="TsHA"/>
									<integer id="set_property" value="Shwn"/>
									<boolean id="dont_set_object" value="true"/>
								</record>
								<record>
									<integer id="get_property" value="objt"/>
									<integer id="set_property" value="Cclu"/>
									<boolean id="dont_set_object" value="true"/>
								</record>
								<record>
									<integer id="get_property" value="Tsre"/>
									<integer id="set_property" value="objt"/>
									<boolean id="dont_set_hint" value="true"/>
								</record>
							</list>
						</record>
					</widget>

					<!-- NO STADIUM INFO -->
					<widget class="text" id="NoSt" alignment="centre">
						<translation id="text" translation_id="436452" type="use" value="Stadium information not available.[COMMENT: line of text saying when we don't have any information on a team's stadium]"/>
						<record id="object_property">
							<integer id="get_property" value="TsHA"/>
							<integer id="set_property" value="hidn"/>
						</record>
					</widget>
				</container>

				<!-- BOTTOM SELECTOR -->
				<container class="team_container_information_overview_panel" file="none">
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>

					<record id="object_property" get_property="objt" set_property="objt"/>

					<container class="tcs_box" gap="8" draw_vertical_dividers="true" priority="1">
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>
						<record id="default_properties" default_width="-1" minimum_width="250"/>

						<!--snapshot selector 1-->
						<container class="client_object_viewer_selector_panel" file="club/club overview selector panel" id="cos2" priority="1" default_widget="upfx" late_loading="true"/>

					</container>

				</container>
			</container>

			<!-- BEST XI -->
			<container id="beXi">
				<translation id="title" translation_id="258247" type="use"/>
				<layout class="stick_to_sides_attachment" apply_to_children="true"/>

				<container class="tcs_box">
					<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

					<container class="tc_best_eleven_panel" id="trbe" file="none">
						<record id="object_property" get_property="objt" set_property="objt"/>
						<boolean id="save_session_state" value="true"/>
						<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="0"/>

						<!-- Starting XI Styles -->
						<flags id="stxi" value="tables/standard/normal"/>
						<!-- Subs Styles -->
						<flags id="subs" value="tables/custom/squad/row/substitute/row"/>
						<!-- divider row appearance -->
						<flags id="divider_appearance" value="tables/standard/divider/row"/>

						<container id="titl">
							<attachment_group class="vertical_arrange" vertical_layout="28, -1" horizontal_gap="0"/>
							<!-- embedded view menu -->
							<container id="btns" navigation_container="true" default_focus="true">
								<attachment_group class="horizontal_arrange" horizontal_alignment="right"/>

								<!-- seasons popup -->
								<widget class="embedded_season_selection_list" id="sesl" width="200" sele="sels" height="30"/>
							</container>

							<container>
								<attachment_group class="horizontal_arrange" horizontal_layout="-1" horizontal_gap="0"/>
								<container>
									<layout class="arrange_vertical_attachment" alignment="bottom,extend" offset="0"/>
									<layout class="stick_to_sides_attachment" alignment="horizontal" apply_to_children="true" inset="0"/>

									<widget class="client_object_list_table" id="awsl" sort_disabled="true" always_use_latest_object_list="true" should_force_refresh="true" navigation_container="true">
										<integer id="contextual_handler_column_id" value="Pnms"/>
										<list id="viewed_property_ids">
											<integer value="Pers"/>
											<integer value="Pbep"/>
											<integer value="Pbea"/>
											<integer value="Pbeg"/>
											<integer value="Pbav"/>
										</list>
									</widget>

									<container navigation_container="true">
										<layout class="arrange_horizontal_attachment" alignment="right" offset="0" gap="0"/>
										<layout class="stick_to_sides_attachment" alignment="bottom" apply_to_children="true" inset="5"/>
										<layout class="fit_children_attachment" alignment="vertical" offset="5"/>

										<widget class="icon_button" auto_size="horizontal" height="30" click_event="ecwn" controller_button="y_button">
											<translation id="text" translation_id="275840" type="use" value="Where Are They Now?"/>
										</widget>
									</container>

								</container>

								<widget class="pitch_with_tactics" id="attp" icon_flags="show_name,use_player_kits_back" player_icon_file="tactics/tactics icon info panel" navigation_container="true">
									<boolean id="vertical" value="true"/>
									<boolean id="pitch_is_read_only" value="true"/>
									<integer id="pitch_vertical_padding" value="20"/>
								</widget>
							</container>
						</container>

						<!-- panel for no data available-->
						<container class="empty_state" id="nb11" header_widget="BShe" navigation_container="true" default_focus="true">
							<widget class="text" id="BShe">
								<translation id="text" translation_id="320160" type="use" value="No records available[COMMENT: team records overview panel, text to display when there are no player records close to being broken]"/>
							</widget>
						</container>

					</container>
				</container>
			</container>

			<container id="co03" icon="tcs/icons/club/Hof">
				<translation id="title" translation_id="247122" type="use" value="Hall of Fame"/>
				<layout class="stick_to_sides_attachment" apply_to_children="true"/>

				<container class="plain_box" appearance="FME/tabbed pane/subtle/paper">
					<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

					<container class="team_container_information_overview_panel" id="csio" file="club/tcs/tcs22v2/hall of fame">
						<record id="object_property" get_property="objt" set_property="objt"/>
					</container>
				</container>
			</container>

			<container id="co04" icon="icons/26px/development">
				<translation id="title" type="use" value="Loans"/>
				<layout class="stick_to_sides_attachment" apply_to_children="true"/>

				<container class="plain_box" appearance="FME/tabbed pane/subtle/paper">
					<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true"/>

					<container class="players_on_loan_panel" file="club/tcs/tcs22v2/loans">
						<record id="object_property" get_property="objt" set_property="objt"/>
					</container>
				</container>
			</container>
		</container>

		<!-- RIGHT -->
		<container>
			<layout class="stick_to_sides_attachment" apply_to_children="true"/>

			<container class="tcs_tabs" id="tsta" default_height="-1" priority="1" default_item="st01" save_default_state="true" save_session_state="true">

				<!-- KEY EMPLOYEES -->
				<container id="st01">
					<translation id="title" translation_id="257300" type="use"/>
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>

					<!-- BOTTOM SELECTOR -->
					<container class="team_container_information_overview_panel" file="none">
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>
						<record id="object_property" get_property="objt" set_property="objt"/>

						<!--snapshot selector 1-->
						<container class="client_object_viewer_selector_panel" file="club/staff selector panel" id="cos1" priority="1" default_widget="pssp" late_loading="true"/>
					</container>
				</container>
								
				<!-- SUPPORTER PROFILES -->
				<container id="st02">
					<translation id="title" translation_id="320609" type="use"/>
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>

					<!-- BOTTOM SELECTOR -->
					<container class="team_container_information_overview_panel" file="none">
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>
						<record id="object_property" get_property="objt" set_property="objt"/>

						<!--snapshot selector 1-->
						<container class="client_object_viewer_selector_panel" file="club/supporter selector panel" id="cos1" priority="1" default_widget="pssp" late_loading="true"/>
					</container>
				</container>

				<!-- LAST XI -->
				<container id="st03">
					<translation id="title" translation_id="344103" type="use"/>
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>

					<container class="tcs_box">
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>

						<!-- BOTTOM SELECTOR -->
						<container class="team_container_information_overview_panel" id="csio" file="club/tcs/overview/last xi">
							<layout class="stick_to_sides_attachment" apply_to_children="true"/>
							<record id="object_property" get_property="objt" set_property="objt"/>
						</container>
					</container>
				</container>
				
				<!-- BOARD MEMBERS -->
				<!-- LAST XI -->
				<container id="st04">
					<translation id="title" translation_id="557622" type="use"/>
					<layout class="stick_to_sides_attachment" apply_to_children="true"/>

					<container class="tcs_box">
						<layout class="stick_to_sides_attachment" apply_to_children="true"/>

						<!-- BOTTOM SELECTOR -->
						<container class="board_profile_panel" id="boar" file="none">
							<record id="object_property" get_property="objt" set_property="objt"/>
							<attachment_group class="vertical_arrange" vertical_alignment="top, extend" vertical_gap="8"/>
							
							<widget class="text" font="title" colour="title" auto_size="vertical">
								<translation id="text" translation_id="557622" type="use" value="Board Members[COMMENT: AREA: Club vision; SUB-AREA: Board profile; FR: 3266; TONE: Formal; CHARLIMIT: None; NOTE: Title for section listing current board members]"/>
							</widget>
				
							<container>
								<attachment_group class="vertical_arrange"/>

								<container class="team_container_staff_details_panel" type="TCsO" file="team/team container staff details board overview panel">
									<record id="object_property" get_property="objt" set_property="objt"/>
									<layout class="stick_to_sides_attachment" alignment="all" inset="0"/>
								</container>
							</container>
						
						</container>
					</container>
				</container>
				
			</container>
		</container>
	</container>
</panel>