<panel navigation_container="true">
	<container id="SUBP" class="contrast_box" red_replacement="bg" >
		<layout class="stick_to_sides_attachment" alignment="all" apply_to_children="true" inset="0" />
		<layout class="stick_to_sides_attachment" alignment="all" inset="0" />
	</container>

	<widget class="popup_button" id="popv" secondary_icon_enabled="false" icon="icons/16px/settings" red_replacement="bg lighter" auto_size="horizontal" height="16" click_event="msis" appearance="buttons/custom/customise panel/button" icon_colour="statman" fixed="true" navigation_focus_target="true">
		<layout class="stick_to_sides_attachment" alignment="right" inset="10"/>
		<layout class="stick_to_sides_attachment" alignment="top" inset="-145"/>
	</widget>

	<!-- Make sure everything is late loading! -->

<!--DEFENSIVE-->
<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
<widget class="client_object_viewer_xml_panel" id="per1" file="player/stats/percentiles/defender" default_item="true" late_loading="true" >
<translation id="subtitle" value="Metric Bars" type="use"/>
<translation id="title" type="use" value="Defender" />
<record id="object_property" get_property="objt" set_property="objt"/>
</widget>

<!--FULL BACK-->
<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
<widget class="client_object_viewer_xml_panel" id="per1" file="player/stats/percentiles/full back" default_item="true" late_loading="true" >
<translation id="subtitle" value="Metric Bars" type="use"/>
<translation id="title" type="use" value="Full Back" />
<record id="object_property" get_property="objt" set_property="objt"/>
</widget>

<!--DEF MIDFIELDER-->
<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
<widget class="client_object_viewer_xml_panel" id="per2" file="player/stats/percentiles/midfielder" default_item="true" late_loading="true" >
<translation id="subtitle" value="Metric Bars" type="use"/>
<translation id="title" type="use" value="Midfielder" />
<record id="object_property" get_property="objt" set_property="objt"/>
</widget>

<!--ATT MIDFIELDER-->
<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
<widget class="client_object_viewer_xml_panel" id="per2" file="player/stats/percentiles/winger" default_item="true" late_loading="true" >
<translation id="subtitle" value="Metric Bars" type="use"/>
<translation id="title" type="use" value="Attacking Midfielders" />
<record id="object_property" get_property="objt" set_property="objt"/>
</widget>

<!--FORWARD-->
<layout class="stick_to_sides_attachment" alignment="all" inset="0" apply_to_children="true" />
<widget class="client_object_viewer_xml_panel" id="per3" file="player/stats/percentiles/attacker" default_item="true" late_loading="true">
<translation id="subtitle" value="Metric Bars" type="use"/>
<translation id="title" type="use" value="Forward" />
<record id="object_property" get_property="objt" set_property="objt"/>
</widget>


</panel>