		<translation id="subtitle" value="Personal" type="use"/>
		<layout class="horizontal_arrange" horizontal_layout="-1,-2,20" offset="0" inset="0" gap="0"/>
			<widget class="label" size="10" font="title" colour="faded text" multiline="true" alignment="left,centre_y,can_scale" >
				<translation id="text" type="use" value="Age" />
			</widget>
		<widget class="formatted_label" font="title" alignment="right,centre_y" colour="text">
			<translation id="format" translation_id="242551" type="use" value="[%number#1] years old" />
