					<widget class="player_additional_stats_panel" id="astD" file="player/player additional stats mini report defending" colour="faded text">
						<record id="object_property" get_property="objt" set_property="objt" />
					</widget>

					<widget class="data_analyst_visualisation_widget" id="visU" hidden="true" visualisation_type="polygon" data_type="player" category="defensive_wide_midfielder" file="player/tcs/polygon nobg" show_mini_visualisation_text="true">
						<record id="object_property" get_property="objt" set_property="objt" />
						<record id="object_property" get_property="PclC" set_property="comp" />
					</widget>
				</container>

			</container>

			<container class="tcs_box">
				<layout class="stick_to_sides_attachment" apply_to_children="true"/>

				<animation class="slide_in_right_animation"/>

				<container class="horizontal_adaptive_container" gap="10" mode="distribute_equally" id="per90">
					<widget class="player_additional_stats_panel" file="player/stats/percentiles/goalkeeper">
						<record id="object_property" get_property="objt" set_property="objt" />
					</widget>
				</container>

			</container>
